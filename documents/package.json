{
  "name": "ef-docs",
  "version": "5.1.0",
  "private": true,
  "description": "Element Framework Documentation",
  "engines": {
    "node": ">=16.11.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "pandora-cmd": "npx -y ./tools/pandora-book-3.0.0-dev.tgz",
    "start": "npm run build:api && concurrently \"npm run pandora-cmd -- build dist --watch\" \"serve dist -s\"",
    "copy:resources": "cp -r resources dist/resources && cp src/theme-loader.js dist/theme-loader.js",
    "build:api": "node ./scripts/element.injector.js",
    "build": "rm -rf build && npm run build:api && npm run pandora-cmd -- build dist --clean && npm run copy:resources"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Refinitiv/refinitiv-ui.git",
    "directory": "documents"
  },
  "keywords": [
    "Element Framework",
    "EF"
  ],
  "author": "Refinitiv",
  "license": "Apache-2.0",
  "devDependencies": {
    "chalk": "^4.1.2",
    "concurrently": "^6.4.0",
    "serve": "^13.0.2"
  },
  "dependencies": {
    "@refinitiv-ui/elements": "^5.10.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0"
  }
}
