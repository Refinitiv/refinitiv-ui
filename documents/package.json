{
  "name": "@refinitiv-ui/docs",
  "version": "7.0.0-next",
  "private": true,
  "description": "Element Framework Documentation",
  "engines": {
    "node": ">=16.11.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start:dev": "concurrently \"pandora src dist --watch\" \"serve dist -s\"",
    "start": "serve dist -s",
    "copy:resources": "cp src/_redirects dist/_redirects && cp -r resources dist/resources",
    "build:api": "lerna run --scope @refinitiv-ui/components api-analyzer --stream && node ./scripts/element.injector.js",
    "build": "rm -rf build && npm run build:api && pandora build dist --clean && npm run copy:resources"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Refinitiv/refinitiv-ui.git",
    "directory": "documents"
  },
  "keywords": [
    "Element Framework",
    "EF"
  ],
  "author": "LSEG",
  "license": "Apache-2.0",
  "devDependencies": {
    "chalk": "^4.1.2",
    "concurrently": "^6.4.0",
    "pandora-book": "file:tools/pandora-book-3.0.0-68.tgz",
    "serve": "^14.0.0"
  },
  "dependencies": {
    "@refinitiv-ui/components": "7.0.0-next.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0"
  }
}
