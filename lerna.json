{
  "packages": ["packages/*", "vscode-extensions/*", "documents"],
  "npmClient": "npm",
  "useWorkspaces": true,
  "version": "independent",
  "command": {
    "publish": {
      "message": "chore(release): publish [skip ci]",
      "registry": "https://registry.npmjs.org"
    }
  },
  "ignoreChanges": ["**/__test__/**", "**/__demo__/**", "**/__snapshots__/**", ".vscode/**"]
}
