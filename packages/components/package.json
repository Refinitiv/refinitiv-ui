{
  "name": "@refinitiv-ui/elements",
  "version": "7.0.0-next.0",
  "description": "Element Framework Elements",
  "author": "LSEG",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "homepage": "https://ui.refinitiv.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:Refinitiv/refinitiv-ui.git",
    "directory": "packages/elements"
  },
  "engines": {
    "node": ">=16.11.0",
    "npm": ">=8.0.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*/index.d.ts",
        "lib/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "default": "./lib/index.js"
    },
    "./version": "./lib/version.js",
    "./icon": "./lib/icon/index.js",
    "./checkbox": "./lib/checkbox/index.js",
    "./sub-label": "./lib/sub-label/index.js",
    "./sub-checkbox": "./lib/sub-checkbox/index.js"
  },
  "scripts": {
    "build": "node cli build --sourceMap --declarationMap",
    "build:prod": "node cli build",
    "start": "node cli start",
    "test": "node cli test",
    "test:watch": "node cli test --watch",
    "test:snapshots": "node cli test --update-snapshots",
    "lint": "node cli lint",
    "lint:fix": "node cli lint --fix",
    "prepublishOnly": "node scripts/release",
    "api-analyzer": "node ../../scripts/release/api-analyzer.js",
    "prepack": "npm run version",
    "version": "node ../../scripts/version"
  },
  "dependencies": {
    "@refinitiv-ui/browser-sparkline": "1.1.8",
    "@refinitiv-ui/halo-theme": "^7.0.0-next.0",
    "d3-interpolate": "^3.0.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@refinitiv-ui/core": "^7.0.0-next.0",
    "@refinitiv-ui/tokens": "^7.0.0-next.0",
    "@refinitiv-ui/demo-block": "^7.0.0-next.0",
    "@refinitiv-ui/i18n": "^7.0.0-next.0",
    "@refinitiv-ui/phrasebook": "^7.0.0-next.0",
    "@refinitiv-ui/test-helpers": "^7.0.0-next.0",
    "@refinitiv-ui/translate": "^7.0.0-next.0",
    "@refinitiv-ui/utils": "^7.0.0-next.0",
    "@types/d3-interpolate": "^3.0.1"
  },
  "peerDependencies": {
    "@refinitiv-ui/core": "^7.0.0-next.0",
    "@refinitiv-ui/i18n": "^7.0.0-next.0",
    "@refinitiv-ui/phrasebook": "^7.0.0-next.0",
    "@refinitiv-ui/translate": "^7.0.0-next.0",
    "@refinitiv-ui/utils": "^7.0.0-next.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
