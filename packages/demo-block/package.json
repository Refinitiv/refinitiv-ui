{
  "name": "@refinitiv-ui/demo-block",
  "version": "5.0.3",
  "description": "Demo page handler for Element Framework elements",
  "main": "./lib/demo-block.js",
  "module": "./lib/demo-block.js",
  "types": "./lib/demo-block.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch --preserveWatchOutput --sourceMap",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "npm run build && concurrently \"npm run build:watch\" \"web-dev-server --config ../../server.config.js --app-index __demo__\"",
    "test": "npm run build && karma start ../../karma.config.js --package=demo-block",
    "test:snapshots": "npm run test -- --snapshots",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sami.int.thomsonreuters.com:elf/refinitiv-ui.git",
    "directory": "packages/demo-block"
  },
  "author": "Refinitiv",
  "license": "Apache-2.0",
  "dependencies": {
    "@refinitiv-ui/core": "^5.0.2",
    "@refinitiv-ui/elemental-theme": "^5.2.0",
    "@refinitiv-ui/halo-theme": "^5.2.0",
    "@refinitiv-ui/solar-theme": "^5.2.0"
  },
  "devDependencies": {
    "@refinitiv-ui/test-helpers": "^5.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
