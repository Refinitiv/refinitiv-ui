{
  "brand": {
    "color": {
      "$value": "{cyan.900}"
    }
  },
  "primary": {
    "color": {
      "$value": "{brand.color}"
    }
  },
  "text": {
    "header": {
      "font-family": {
        "$value": "{font.family.header}"
      },
      "color": {
        "$value": "{grey.900}"
      },
      "size": {
        "$value": "{font.size.x-large}"
      }
    },
    "sub-header": {
      "font-family": {
        "$value": "{font.family.header}"
      },
      "color": {
        "$value": "{grey.900}"
      },
      "size": {
        "$value": "{font.size.large}"
      }
    },
    "body": {
      "font-family": {
        "$value": "{font.family.body}"
      },
      "color": {
        "$value": "{grey.800}"
      },
      "hover": {
        "color": {
          "$value": "{grey.900}"
        }
      },
      "size": {
        "$value": "{font.size.small}"
      }
    },
    "selection": {
      "color": {
        "$value": "{text.body.color}"
      },
      "background": {
        "color": {
          "$value": "{primary.color}"
        }
      }
    },
    "disabled": {
      "color": {
        "$value": "{grey.500}"
      }
    }
  },
  "background": {
    "color": {
      "$value": "{grey.100}"
    }
  },
  "font": {
    "size": {
      "$type": "dimension",
      "xx-small": {
        "$value": "10px"
      },
      "x-small": {
        "$value": "12px"
      },
      "small": {
        "$value": "14px"
      },
      "medium": {
        "$value": "16px"
      },
      "large": {
        "$value": "20px"
      },
      "x-large": {
        "$value": "24px"
      },
      "xx-large": {
        "$value": "36px"
      },
      "xxx-large": {
        "$value": "48px"
      }
    },
    "family": {
      "$type": "fontFamily",
      "header": {
        "$value": [
          "Georgia",
          "serif"
        ]
      },
      "body": {
        "$value": [
          "Verdana",
          "sans-serif"
        ]
      }
    },
    "weight": {
      "default": {
        "$type": "fontWeight",
        "$value": 500
      },
      "bold": {
        "$type": "fontWeight",
        "$value": 600
      }
    }
  },
  "size": {
    "$type": "dimension",
    "form-border": {
      "$value": "1px"
    },
    "xxx-small": {
      "$value": "8px"
    },
    "xx-small": {
      "$value": "10px"
    },
    "x-small": {
      "$value": "16px"
    },
    "small": {
      "$value": "20px"
    },
    "medium": {
      "$value": "24px"
    },
    "large": {
      "$value": "30px"
    },
    "x-large": {
      "$value": "36px"
    },
    "xx-large": {
      "$value": "48px"
    },
    "xxx-large": {
      "$value": "72px"
    },
    "xxxx-large": {
      "$value": "144px"
    }
  },
  "space": {
    "$type": "dimension",
    "xxx-small": {
      "$value": "2px"
    },
    "xx-small": {
      "$value": "4px"
    },
    "x-small": {
      "$value": "8px"
    },
    "small": {
      "$value": "12px"
    },
    "medium": {
      "$value": "16px"
    },
    "large": {
      "$value": "20px"
    },
    "x-large": {
      "$value": "28px"
    },
    "xx-large": {
      "$value": "36px"
    },
    "xxx-large": {
      "$value": "48px"
    }
  },
  "border-radius": {
    "$type": "dimension",
    "sharp": {
      "$value": "0px"
    },
    "pill": {
      "$value": "20rem"
    },
    "curve": {
      "$value": "4px"
    },
    "circle": {
      "$value": "50%"
    }
  },
  "transition": {
    "$type": "duration",
    "quickly": {
      "$value": "200ms"
    },
    "slowly": {
      "$value": "200ms"
    }
  },
  "control": {
    "color": {
      "$value": "{text.body.color}"
    },
    "height": {
      "$value": "{size.medium}"
    },
    "width": {
      "$value": "{size.xxxx-large}"
    },
    "padding": {
      "$value": "{space.xx-small}"
    },
    "radius": {
      "$value": "{border-radius.sharp}"
    },
    "border-style": {
      "$type": "strokeStyle",
      "$value": "solid"
    },
    "border-width": {
      "$value": "{size.form-border}"
    },
    "border": {
      "$type": "border",
      "$value": {
        "color": "{grey.400}",
        "width": "{control.border-width}",
        "style": "{control.border-style}"
      }
    },
    "background": {
      "color": {
        "$value": "{background.color}"
      }
    },
    "disabled": {
      "color": {
        "$value": "{text.disabled.color}"
      },
      "background": {
        "color": {
          "$value": "{control.background.color}"
        }
      },
      "border": {
        "color": {
          "$type": "color",
          "$value": "{grey.300}"
        }
      }
    },
    "readonly": {
      "color": {
        "$value": "{text.disabled.color}"
      },
      "background": {
        "color": {
          "$value": "{control.background.color}"
        }
      },
      "border": {
        "color": {
          "$value": "{grey.300}"
        }
      }
    },
    "focus": {
      "color": {
        "$value": "{primary.color}"
      },
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.focus.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      }
    },
    "hover": {
      "color": {
        "$value": "{text.body.hover.color}"
      },
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.focus.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      }
    },
    "error": {
      "color": {
        "$type": "color",
        "$value": "red"
      },
      "hover": {
        "color": {
          "$type": "color",
          "$value": "pink"
        }
      }
    },
    "warning": {
      "color": {
        "$type": "color",
        "$value": "rgb(204, 160, 0)"
      },
      "hover": {
        "color": {
          "$type": "color",
          "$value": "rgb(255, 233, 153)"
        }
      }
    }
  },
  "field": {
    "color": {
      "$value": "{control.color}"
    },
    "background": {
      "color": {
        "$value": "{background.color}"
      }
    },
    "height": {
      "$value": "{control.height}"
    },
    "width": {
      "$value": "{control.width}"
    },
    "padding": {
      "$value": "{control.padding}"
    },
    "radius": {
      "$value": "{control.radius}"
    },
    "border": {
      "$value": "{control.border}"
    },
    "focus": {
      "border": {
        "$value": "{control.focus.border}"
      }
    },
    "icon": {
      "focus": {
        "border": {
          "$value": "{control.focus.border}"
        },
        "radius": {
          "$value": "{control.radius}"
        }
      }
    },
    "disabled": {
      "color": {
        "$value": "{control.disabled.color}"
      },
      "background": {
        "color": {
          "$value": "{control.disabled.background.color}"
        }
      },
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.disabled.border.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      }
    },
    "readonly": {
      "color": {
        "$value": "{control.readonly.color}"
      },
      "background": {
        "color": {
          "$value": "{control.readonly.background.color}"
        }
      },
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.readonly.border.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      }
    },
    "hover": {
      "border": {
        "$value": "{control.hover.border}"
      }
    },
    "error": {
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.error.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      },
      "hover": {
        "border": {
          "$type": "border",
          "$value": {
            "color": "{control.error.hover.color}",
            "width": "{control.border-width}",
            "style": "{control.border-style}"
          }
        }
      }
    },
    "warning": {
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.warning.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      },
      "hover": {
        "border": {
          "$type": "border",
          "$value": {
            "color": "{control.warning.hover.color}",
            "width": "{control.border-width}",
            "style": "{control.border-style}"
          }
        }
      }
    }
  },
  "checkbox": {
    "color": {
      "$value": "{control.color}"
    },
    "size": {
      "$value": "{control.height}"
    },
    "border": {
      "$value": "{control.border}"
    },
    "radius": {
      "$value": "{control.radius}"
    },
    "hover": {
      "color": {
        "$value": "{control.hover.color}"
      },
      "border": {
        "$value": "{control.hover.border}"
      }
    },
    "focus": {
      "border": {
        "$value": "{control.focus.border}"
      }
    },
    "disabled": {
      "color": {
        "$value": "{control.disabled.color}"
      },
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.disabled.border.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      }
    },
    "readonly": {
      "color": {
        "$value": "{control.readonly.color}"
      },
      "border": {
        "$type": "border",
        "$value": {
          "color": "{control.readonly.border.color}",
          "width": "{control.border-width}",
          "style": "{control.border-style}"
        }
      }
    }
  },
  "label": {
    "error": {
      "color": {
        "$value": "{control.error.color}"
      }
    },
    "warning": {
      "color": {
        "$value": "{control.warning.color}"
      }
    }
  },
  "overlay": {
    "background": {
      "color": {
        "$value": "{background.color}"
      }
    },
    "opacity": {
      "$value": "0.6"
    },
    "box-shadow": {
      "$type": "shadow",
      "$value": {
        "color": "rgb(0 0 0 / 25%)",
        "offsetX": "0px",
        "blur": "2px",
        "spread": "8px"
      }
    }
  },
  "item": {
    "color": {
      "$value": "{text.body.color}"
    },
    "min-height": {
      "$value": "{control.height}"
    },
    "icon": {
      "margin": {
        "$value": "0.5em"
      }
    },
    "padding": {
      "$value": "{control.padding}"
    },
    "selected": {
      "color": {
        "$value": "white"
      }
    },
    "focus": {
      "color": {
        "$value": "white"
      },
      "border": {
        "$value": "{control.focus.border}"
      },
      "background": {
        "color": {
          "$value": "{primary.color}"
        }
      }
    },
    "header": {
      "color": {
        "$value": "{grey.700}"
      },
      "background": {
        "color": {
          "$value": "{background.color}"
        }
      },
      "font": {
        "size": {
          "$type": "dimension",
          "$value": "0.8em"
        },
        "weight": {
          "$type": "fontWeight",
          "$value": "{font.weight.bold}"
        }
      }
    },
    "divider": {
      "background": {
        "$value": "linear-gradient(rgb(64, 64, 64), rgb(64, 64, 64)) 0px center / calc(100% - 0px) 1px no-repeat"
      },
      "height": {
        "$value": "5px"
      }
    },
    "disabled": {
      "color": {
        "$value": "{text.disabled.color}"
      }
    }
  },
  "select": {
    "list": {
      "max-width": {
        "$value": "300px"
      }
    }
  }
}
