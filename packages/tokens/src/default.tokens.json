        "default": {
          "$value": "{color.silver}"
        },
        "hover": {
          "$value": "{color.white}"
        },
        "focused": {
          "$value": "{color.lights-out}"
        },
        "pressed": {
          "$value": "{color.white}"
        }
      },
      "secondary": {
        "default": {
          "$value": "{color.lights-out}"
        },
        "hover": {
          "$value": "{color.white}"
        },
        "focused": {
          "$value": "{color.lights-out}"
        },
        "pressed": {
          "$value": "{color.white}"
        }
      }
