  "width": {
    "$type": "dimension",
    "fixed0": {
      "$value": "0px"
    },
    "fixed1": {
      "$value": "1px"
    },
    "fixed2": {
      "$value": "2px"
    },
    "fixed3": {
      "$value": "3px"
    },
    "010": {
      "$value": "4px"
    },
    "020": {
      "$value": "8px"
    },
    "030": {
      "$value": "12px"
    },
    "040": {
      "$value": "16px"
    },
    "050": {
      "$value": "24px"
    }
  },
  "shadow": {
    "$type": "shadow",
    "fixed1": {
      "$value": {
        "color": "rgba(0, 0, 0, 0.5)",
        "offsetX": "{width.fixed1}",
        "offsetY": "{width.fixed1}",
        "blur": "{width.fixed1}",
        "spread": "{width.fixed1}"
      }
    }
  },
  "base": {
    "bg": {
      "$type": "color",
      "primary": {
        "$value": "{color.white}"
      },
      "secondary": {
        "$value": "{color.silver}"
      },
      "emphasis": {
        "$value": "{color.lights-out}"
      }
    },
    "content": {
      "$type": "color",
      "primary": {
        "$value": "{color.lights-out}"
      }
    }
  },
  "action": {
    "bg": {
      "$type": "color",
      "primary": {
        "default": {
          "$value": "{color.lights-out}"
        },
        "hover": {
          "$value": "{color.persian-blue}"
        },
        "focused": {
          "$value": "{color.white}"
        },
        "pressed": {
          "$value": "{color.torea-bay}"
        }
      },
      "secondary": {
        "default": {
          "$value": "{color.alabaster}"
        },
        "hover": {
          "$value": "{color.persian-blue}"
        },
        "focused": {
          "$value": "{color.white}"
        },
        "pressed": {
          "$value": "{color.torea-bay}"
        }
      }
    },
    "border": {
      "$type": "border",
      "primary": {
        "default": {
          "$value": {
            "color": "{color.lights-out}",
            "width": "{width.fixed1}",
            "style": "solid"
          }
        },
        "hover": {
          "$value": {
            "color": "{color.persian-blue}",
            "width": "{width.fixed1}",
            "style": "solid"
          }
        },
        "focused": {
          "$value": {
            "color": "{color.lights-out}",
            "width": "1px",
            "style": "solid"
          }
        },
        "pressed": {
          "$value": {
            "color": "{color.torea-bay}",
            "width": "{width.fixed1}",
            "style": "solid"
          }
        }
      },
      "secondary": {
