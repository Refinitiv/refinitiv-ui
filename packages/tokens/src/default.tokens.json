          "$value": {
            "color": "{color.scorpion}",
            "width": "{width.fixed1}",
            "style": "solid"
          }
        },
        "hover": {
          "$value": {
            "color": "{color.persian-blue}",
            "width": "{width.fixed1}",
            "style": "solid"
          }
        },
        "focused": {
          "$value": {
