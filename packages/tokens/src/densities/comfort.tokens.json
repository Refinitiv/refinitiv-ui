{
  "token-name": {
    "$value": "comfort",
    "$description": "use for testing purpose only"
  },
  "space": {
    "$type": "dimension",
    "010": {
      "$value": "8px"
    },
    "020": {
      "$value": "12px"
    },
    "030": {
      "$value": "16px"
    }
  },
  "code-only": {
