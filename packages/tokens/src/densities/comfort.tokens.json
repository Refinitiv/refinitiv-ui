      "padding-horizontal": {
        "default": {
          "$value": "{space.030}"
        }
      },
      "padding-vertical": {
        "default": {
          "$value": "{space.020}"
        }
      },
      "item-spacing": {
        "default": {
          "$value": "{space.010}"
        }
      },
      "line-height": {
        "default": {
          "$value": "{copy.md-loud.line-height}"
        }
      }
    }
